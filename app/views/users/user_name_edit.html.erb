<% provide(:title, "ユーザー名編集") %>
<h1>ユーザー名編集</h1>
<%= form_with model: @user, url: user_name_update_path do |f| %>
<%= render "shared/error", obj: @user %>
  <div>
    <%= f.label :ユーザー名 %><br>
    <%= f.text_field :user_name %>
  </div>
  <div>
    <%= f.submit "更新" %>
  </div>
<% end %>
<%= link_to "戻る", :back %><br>
